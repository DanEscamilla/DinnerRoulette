{
  "name": "Dinner Roulette",
  "description": "Extension to pick a restaurant in the UberEats website",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_title": "Roll the roulette!",
    "default_popup": "/build/index.html",
    "default_icon": {
      "16": "/images/16.png",
      "32": "/images/32.png",
      "48": "/images/48.png",
      "128": "/images/128.png"
    }
  },
  "icons": {
    "16": "/images/16.png",
    "32": "/images/32.png",
    "48": "/images/48.png",
    "128": "/images/128.png"
  }
}
